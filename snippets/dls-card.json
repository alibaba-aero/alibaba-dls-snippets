{
    "DLS Button [without any props]": {
        "prefix": "!abtn",
        "body": "<a-button>${1}</a-button>",
        "description": "DLS Button without any props"
    },
    "DLS Button [with every prop]": {
        "prefix": "!abtn-full",
        "body": "<a-button variant=\"${1}\" size=\"${2}\" block disabled=\"${3}\" loading=\"${4}\">${5}</a-button>",
        "description": "DLS Button Including every possible props"
    },
    "DLS Button [with variant and @click]": {
        "prefix": "!abtn-simple",
        "body": "<a-button variant=\"${1}\" @click=\"${2}\">${3}</a-button>",
        "description": "DLS Button with variant and click event handler"
    },
}
